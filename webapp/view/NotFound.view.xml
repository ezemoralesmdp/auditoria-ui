<mvc:View
    controllerName="auditui.auditoriaui.controller.NotFound"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    displayBlock="true"
    height="100%"
    busyIndicatorDelay="0"
>
    <IllustratedMessage
        illustrationType="sapIllus-PageNotFound"
        title="Error 404"
        description="Ruta no encontrada"
    >
        <additionalContent>
            <Button 
                text="Volver al inicio" 
                press="onReturnMainButton"
            />
        </additionalContent>
    </IllustratedMessage>
</mvc:View>